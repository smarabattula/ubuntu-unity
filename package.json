{
  "name": "tbt-education-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.jsx",
  "scripts": {
    "dev": "vite",
    "start": "NODE_OPTIONS=--openssl-legacy-provider react-scripts start --public-url=/",
    "build": "vite build && npm run build:css",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:css": "tailwindcss build -i src/styles/tailwind.css -o dist/output.css",
    "watch:css": "tailwindcss build -i src/styles/tailwind.css -o dist/output.css --watch"
  },
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.7",
    "axios": "^1.6.5",
    "flowbite-react": "^0.7.2",
    "framer-motion": "^10.18.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.2",
    "react-scripts": "^5.0.1",
    "wouter": "^2.12.1",
    "tw-elements-react": "^1.0.0-alpha2"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.12"
  }
}
